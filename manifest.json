{
  "manifest_version":2,
  "name": "Youtube Browser",
  "version" : "0.0.1",
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"],
      "js": ["content.js"]
    },
    {
      "js": [ "EmptyIframeInjection.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
    }
],
  "background":{
    "scripts": ["background.js"]
  }
}
